{
  "author": "The Paypr Company",
  "name": "@paypr/mui5-common-components",
  "description": "Material UI Common Components",
  "license": "MIT",
  "version": "2.0.1",
  "homepage": "https://paypr.money/",
  "repository": {
    "type": "git",
    "url": "https://github.com/paypr/mui5-common-components"
  },
  "bugs": {
    "url": "https://github.com/paypr/mui5-common-components/issues"
  },
  "files": [
    "/dist"
  ],
  "types": "dist/**/*.d.ts",
  "scripts": {
    "clean": "rm -rf dist && rm -rf out",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "copy-assets": "copyfiles -u 1 src/**/*.svg dist",
    "rebuild": "pnpm run clean && npm run build",
    "test": "jest",
    "posttest": "pnpm run lint",
    "lint": "eslint",
    "pretty": "pretty-quick",
    "release": "standard-version",
    "prepublishOnly": "npm run rebuild && npm run copy-assets && npm run release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --ignore-unknown --write ${staged}",
      "pre-push": "npm run lint"
    }
  },
  "packageManager": "pnpm@9.15.9+sha512.68046141893c66fad01c079231128e9afb89ef87e2691d69e4d40eee228988295fd4682181bae55b58418c3a253bde65a505ec7c5f9403ece5cc3cd37dcf2531",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "29.5.14",
    "@types/node": "22.18.8",
    "@types/react": "18.3.25",
    "@types/react-dom": "18.3.7",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "copyfiles": "2.4.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.57.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "prettier": "3.6.2",
    "pretty-quick": "3.3.1",
    "standard-version": "9.5.0",
    "ts-jest": "29.4.4",
    "ts-node": "10.9.2",
    "tsutils": "3.21.0",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "csstype": "^3.1.3",
    "@emotion/react": "^11.11.4",
    "@emotion/serialize": "^1.3.3",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.13.7",
    "@mui/material": "^5.13.7",
    "@mui/system": "^5.13.7",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-number-format": "^5.2.2",
    "react-router-dom": "^6.14.1"
  },
  "engines": {
    "npm": ">=7.20",
    "node": ">=16.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "issueUrlFormat": "https://github.com/paypr/mui5-common-components/issues/{{id}}",
    "issuePrefixes": [
      "#"
    ],
    "scripts": {
      "postchangelog": "npx prettier --write CHANGELOG.md"
    }
  }
}
